{
  "expo": {
    "name": "pin-it-react-native",
    "slug": "pin-it-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyC1g8jOow_Tw-DZ2JQQu-1J7YiBh-Jnun4"
        }
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
